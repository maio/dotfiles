set show-all-if-ambiguous on
set completion-ignore-case on
set bell-style none
$if Bash
    Space: magic-space
$endif

set convert-meta off
set input-meta on
set output-meta on

"\ep": history-search-backward
"\en": history-search-forward
# IJKL IDEA plugin emulation
"\el": forward-word
"\ej": backward-word
"\ei": previous-history
"\ek": next-history